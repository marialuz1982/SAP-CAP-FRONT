<mvc:View controllerName="biblioteca.controller.Biblioteca"
    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content >
            <Table id="idProductsTable" 
                inset="false" items="{/SocioSet}">               		
                
                <columns>
                    <Column>
                        <Text text="DNI" />
                    </Column>
                    <Column>
                        <Text text="Nombre y Apellido" />
                    </Column>
                    <Column>
                        <Text text="Direccion" />
                    </Column>
                    <Column>
                        <Text text="Telefono" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem vAlign="Middle">
                        <cells>					
                            <Text text="{Dni}" />						
                            <Text text="{Nombre}"  />						
                            <Text text="{Direccion}"  />						
                            <Text text="{Telefono}"  />				
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>

        </content>
    </Page>
</mvc:View>
